<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/StartLayout" android:layout_height="fill_parent"
	android:layout_width="fill_parent" android:background="@drawable/bg_generic"
	android:clipChildren="false">








	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/Title_Word"
		android:orientation="horizontal" android:layout_marginTop="10dp"
		android:layout_marginLeft="140dp">
		<ImageView android:layout_alignParentRight="true"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:background="@drawable/title_word_w" android:id="@+id/Title_Word_W"></ImageView>
		<ImageView android:layout_alignParentRight="true"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:background="@drawable/title_word_o" android:id="@+id/Title_Word_O"></ImageView>
		<ImageView android:layout_alignParentRight="true"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:background="@drawable/title_word_r" android:id="@+id/Title_Word_R"></ImageView>
		<ImageView android:layout_alignParentRight="true"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
			android:background="@drawable/title_word_d" android:id="@+id/Title_Word_D"
			android:layout_marginTop="-7dp"></ImageView>
	</LinearLayout>
	<ImageView android:id="@+id/Title_Title"
		android:layout_alignParentRight="true" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:background="@drawable/title_frenzy"
		android:layout_marginTop="50dp" android:layout_marginRight="40dp"></ImageView>
	<ImageButton android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/Title_AboutUs"
		android:layout_alignParentRight="true"
		android:layout_alignParentBottom="true" android:layout_marginRight="5dp"
		android:layout_marginBottom="5dp" android:background="@drawable/title_logo_mini_selector"></ImageButton>
	<TextView android:text="Play" android:id="@+id/Title_PlayText"
		android:textSize="42dp" android:textColor="@color/teamB_text"
		android:gravity="center_vertical" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_marginLeft="100dp"
		android:layout_marginTop="140dp"></TextView>
	<TextView android:textSize="42dp" android:text="Buzz"
		android:textColor="@color/teamC_text" android:id="@+id/Title_BuzzText"
		android:gravity="center_vertical" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_marginLeft="100dp"
		android:layout_marginTop="210dp"></TextView>
	<TextView android:textSize="42dp" android:id="@+id/Title_SettingsText"
		android:text="Settings" android:textColor="@color/teamD_text"
		android:gravity="center_vertical" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_marginLeft="100dp"
		android:layout_marginTop="277dp"></TextView>
	<TextView android:textSize="42dp" android:id="@+id/Title_RulesText"
		android:text="Rules" android:textColor="@color/teamA_text"
		android:gravity="center_vertical" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:layout_marginLeft="100dp"
		android:layout_marginTop="350dp"></TextView>
	<LinearLayout android:layout_width="wrap_content"
		android:orientation="horizontal" android:layout_height="fill_parent"
		android:id="@+id/Title_Banner">
		<View android:layout_height="wrap_content" android:background="@color/genericBG_trim"
			android:id="@+id/Title_Banner_Trim1" android:layout_width="5dp"></View>
		<View android:id="@+id/Title_Banner_BG" android:layout_height="fill_parent"
			android:layout_width="60dp" android:background="@color/black"></View>
		<View android:layout_height="wrap_content" android:background="@color/genericBG_trim"
			android:id="@+id/Title_Banner_Trim2" android:layout_width="5dp"></View>
	</LinearLayout>
	<LinearLayout android:id="@+id/Title_Menu"
		android:orientation="vertical" android:paddingLeft="25dp"
		android:layout_width="wrap_content" android:clipChildren="false"
		android:layout_height="fill_parent">
		<ImageButton android:id="@+id/Title_PlayButton"
			android:background="@drawable/title_play_selector"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_marginBottom="-10dp" android:layout_marginTop="120dp"></ImageButton>

		<ImageButton android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:background="@drawable/title_buzzer_selector"
			android:id="@+id/Title_BuzzButton" android:layout_marginBottom="-10dp"></ImageButton>
		<ImageButton android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:background="@drawable/title_settings_selector"
			android:id="@+id/Title_SettingsButton" android:layout_marginBottom="-10dp"></ImageButton>
		<ImageButton android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:background="@drawable/title_rules_selector"
			android:id="@+id/Title_RulesButton"></ImageButton>
	</LinearLayout>










</RelativeLayout>
